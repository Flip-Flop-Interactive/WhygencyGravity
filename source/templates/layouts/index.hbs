{{>header}}

	<nav id="navigation" class="navbar navbar-default navbar-fixed-top" role="navigation" data-200-top="top: -60px;" data-top="top: 0;" data-anchor-target="#manifesto">
		<div class="navbar-header">
			<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-mechanism">
				<span class="sr-only">Toggle navigation</span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>
			<a class="navbar-brand" href="#">
				<p><span class="logo"><strong>WHYGENCY</strong>GRAVITY</span></p>
			</a>
		</div>
		<div class="navbar-collapse collapse" id="navbar-mechanism">
			<ul class="nav navbar-nav navbar-right">
				{{#compose src="paragraphs/*.md"}}
				<li>
					<a href="#{{ @id }}">{{ @title }}</a>
				</li>
				{{/compose}}
				<li>
					<a href="#contact">Kontakt</a>
				</li>
			</ul>
		</div>
	</nav>

	<section id="splash" class="slide">
		<div class="background" data-center="background-position: 50% 0px;" data-top-bottom="background-position: 50% -200px;" data-anchor-target="#splash"></div>
		<div class="logo" data-center="opacity: 1;" data-center-bottom="opacity: 0;" data-anchor-target="#splash">
			{{>logo}}
		</div>
		<div class="indicator" data-center="opacity: 1;" data--100-center="opacity: 0;" data-anchor-target="#splash">
			<i class="fa fa-chevron-down fa-2x"></i>
		</div>
	</section>

	<section id="intro" class="slide">
		<div class="content" data-top="opacity: 1; margin-top: -15px;" data--200-top="opacity: 0; margin-top: -35px;" data-anchor-target="#intro">

			<div class="mask">
				<p data-150-center="margin-top: 80px;" data-100-center="margin-top: 0px;" data-anchor-target="#intro"><strong>Innovativ strategi   <span class="orange">&gt;</span>  Dectide&Do  <span class="orange">&lt;</span>  Kreativ Produktion</strong><br/>Om någon frågar i hissen så säg bara: ’Erfaren strategi och produktion under samma tak. Med en egen metod och ett flexibelt närverk.</p>	
			</div>
			
			<hr data-200-center="width: 0px;" data-150-center="width: 440px;" data-anchor-target="#intro" />
			
			<div class="mask">
				<p data-150-center="margin-top: -20px;" data-100-center="margin-top: 20px;" data-anchor-target="#intro">Vårt löfte: ’Ge alla på Future Revenue Department en orättvis fördel</p>	
			</div>
		</div>
	</section>

	<section id="manifesto">
		{{#compose src="paragraphs/*.md" markdown="true"}}
		<article id="{{ @id }}">
			<div class="background" style="background-image: url( '{{ @image }}' );" data-bottom-top="background-position: 50% 100px;" data-top-bottom="background-position: 50% -100px;" data-anchor-target="#{{ @id }}"></div>
			<div class="container">
				<div class="col-md-6 col-md-offset-3">
					{{{ @content }}}
				</div>
			</div>
		</article>
		{{/compose}}
	</section>

